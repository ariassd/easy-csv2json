{
  "name": "easy-csv2json",
  "version": "1.0.1",
  "description": "Easiest way to convert CSV files to JSON",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ariassd/easy-csv2json.git"
  },
  "keywords": [
    "csv",
    "json",
    "file"
  ],
  "author": "Luis Arias <ariassd@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ariassd/easy-csv2json",
  "devDependencies": {
    "njscommon": "npm:@nestjs/common@^7.3.2",
    "prettier": "^2.2.0",
    "rxjs": "^6.6.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {},
  "files": [
    "lib/**/*"
  ]
}
